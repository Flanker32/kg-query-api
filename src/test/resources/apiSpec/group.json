  "schema:name": "Species",
  "fields": [
    {
      "fieldname": "query:grouped",
      "label": "Datasets",
      "relative_path": [
        {
          "@id": "minds:species",
          "reverse": true
        },
        {
          "@id": "minds:subjects",
          "reverse": true
        },
        {
          "@id": "minds:specimen_group",
          "reverse": true
        }
      ],
      "grouped_instances": "query:instances",
      "fields": [
        {
          "fieldname": "query:name",
          "relative_path": "schema:name"
        },
        {
          "fieldname": "query:componentName",
          "group_by": true,
          "relative_path": [
            "minds:component",
            "schema:name"
          ]
        }
      ]
    }
  ]
